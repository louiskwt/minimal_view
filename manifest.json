{
    "manifest_version": 2,
    "name": "Minimal View",
    "version": "0.2",
    "description": "A Simple Ad blocker created by Louis T",
    "permissions": ["activeTab", "webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["script.js"]
    },
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}